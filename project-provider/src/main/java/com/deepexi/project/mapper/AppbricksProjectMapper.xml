<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.deepexi.project.mapper.AppbricksProjectMapper">
    <resultMap type="com.deepexi.project.domain.dto.AppbricksProjectDto" id="AppbricksProjectDtoResult">
        <result property="id" column="id" />
        <result property="createPersonId" column="create_person_id" />
        <result property="createTime" column="create_time" />
        <result property="name" column="name" />
        <result property="projectNo" column="project_no" />
        <result property="updatePersonId" column="update_person_id" />
        <result property="updateTime" column="update_time" />
        <result property="systemProject" column="system_project" />
        <result property="userSource" column="user_source" />
        <result property="tenantCode" column="tenant_code" />
        <result property="status" column="status" />
        <result property="attribute1" column="attribute1" />
        <result property="dr" column="dr" />
    </resultMap>
    <select id="findList" resultType="com.deepexi.project.domain.eo.AppbricksProject">
        SELECT * FROM appbricks_project WHERE 1=1
    </select>

    <update id="deleteByIds">
        UPDATE appbricks_project set dr=1 where id in
        <foreach collection="ids" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </update>
</mapper>